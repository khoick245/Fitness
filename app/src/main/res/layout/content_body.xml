<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cecs492b.fitnessjournal.Body"
    tools:showIn="@layout/activity_body"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:id="@+id/trapsButton"
        android:layout_width="170dp"
        android:layout_height="84dp"
        android:background="@null"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/head_image"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="21dp"
        android:contentDescription="" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Front Body"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="4dp"
        android:contentDescription="" />

    <ImageButton
        android:id="@+id/chestButton"
        android:layout_width="113dp"
        android:background="@null"
        android:contentDescription=""
        android:scaleType="fitXY"
        android:layout_height="53dp"
        app:srcCompat="@drawable/chest"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="105dp" />

    <ImageButton
        android:id="@+id/rightArm"
        android:layout_width="40dp"
        android:background="@null"
        android:layout_height="265dp"
        app:srcCompat="@drawable/left_arm"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="81dp" />

    <ImageButton
        android:id="@+id/leftArm"
        android:layout_width="22dp"
        android:layout_height="262dp"
        app:srcCompat="@drawable/right_arm"
        android:background="@null"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteX="245dp"
        tools:layout_editor_absoluteY="84dp" />

    <ImageButton
        android:id="@+id/absButton"
        android:layout_width="100dp"
        android:layout_height="120dp"
        app:srcCompat="@drawable/abs"
        android:background="@null"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteY="158dp"
        tools:layout_editor_absoluteX="142dp" />

    <ImageButton
        android:id="@+id/legsButton"
        android:layout_width="117dp"
        android:layout_height="234dp"
        app:srcCompat="@drawable/legs"
        tools:layout_editor_absoluteY="271dp"
        android:scaleType="fitXY"
        android:background="@null"
        tools:layout_editor_absoluteX="128dp" />
</android.support.constraint.ConstraintLayout>
